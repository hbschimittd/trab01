UPDATE USUARIO
  SET USUARIO.NOME = 'Helio'
WHERE USUARIO.CODIGO = 2;

UPDATE USUARIO
SET USUARIO.E_MAIL = 'hbschimittd@gmail.com'
WHERE USUARIO.CODIGO = 1;

UPDATE EXERCICIO
SET EXERCICIO.NOME = 'Supino'
WHERE EXERCICIO.CODIGO = 4;

UPDATE FICHA_EXERCICIO
SET FICHA_EXERCICIO.QUANTIDADE = 25
WHERE FICHA_EXERCICIO.CODIGO = 5;

UPDATE FICHA
SET FICHA.PESO = 78,5
WHERE FICHA.CODIGO = 4;

UPDATE USUARIO_AVALIACAO
SET USUARIO_AVALIACAO.IMC = 18.5
WHERE USUARIO_AVALIACAO.CODIGO = 4
